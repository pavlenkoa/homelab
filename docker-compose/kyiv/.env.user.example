# User Configuration for Kyiv Environment
# Copy to .env.user and customize

# Domain configuration
DOMAIN_NAME=example.com

# Timezone
TIMEZONE=Europe/Kyiv

# Monitoring Configuration
LOCATION=kyiv
INSTANCE_NAME=kyiv-main

# MikroTik Router Configuration
KYIV_ROUTER1_NAME=kyiv-main
KYIV_ROUTER1_IP=192.168.1.1
KYIV_ROUTER1_PASSWORD=your_mikrotik_prometheus_password

KYIV_ROUTER2_NAME=kyiv-secondary
KYIV_ROUTER2_IP=192.168.1.2
KYIV_ROUTER2_PASSWORD=your_mikrotik_prometheus_password

# Transmission Configuration  
TRANSMISSION_ADDR=http://localhost:9091
WEB_ADDR=0.0.0.0:19091
WEB_PATH=/metrics
